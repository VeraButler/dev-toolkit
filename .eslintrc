{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "no-multiple-empty-lines": [
      "error", {
        "max": 1
      }
    ],
    "import/no-unresolved": [
      2, {
        "ignore": ['\~*']
      }
    ]
  },

  "globals": {
    "app": true,
    "If": true,
    "Else": true
  },
  "env": {
    "mocha": true
  }
}
