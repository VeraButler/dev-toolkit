{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "no-multiple-empty-lines": [
      2, {
        "max": 1
      }
    ],
    "no-debugger": [
      1
    ],
    "react/require-render-return": [
      0
    ]
  },

  "globals": {
    "app": true,
    "If": true,
    "Else": true
  }
}
